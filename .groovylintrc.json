{
    "extends": "recommended",
    "rules": {
        /*"FileEndsWithoutNewline": {
            "enabled": false
        },*/
        "GroovyLintWaiting": {
            "enabled": false
        },
        "LineLength": {
            "enabled": false
        },
        "MethodParameterTypeRequired": {
            "enabled": false
        },
        "MethodReturnTypeRequired": {
            "enabled": false
        },
        "MissingBlankLineAfterImports": {
            "enabled": false
        },
        "NglParseError": {
            "enabled": false
        },
        "NoDef": {
            "enabled": false
        },/*
        "ParameterName": {
            "enabled": false
        },
        "SpaceAfterCatch": {
            "enabled": false
        },
        "SpaceAfterClosingBrace": {
            "enabled": false
        },
        "SpaceAfterSwitch": {
            "enabled": false
        },
        "SpaceBeforeOpeningBrace": {
            "enabled": false
        },
        "TrailingWhitespace": {
            "enabled": false
        },
        "UnnecessaryCatchBlock": {
            "enabled": false
        },
        "UnnecessaryGString": {
            "enabled": false
        },*/
        "UnnecessaryObjectReferences": {
            "enabled": false
        },
        "UnusedImport": {
            "enabled": false
        },
        "UnusedMethodParameter": {
            "enabled": false
        },
        "VariableTypeRequired": {
            "enabled": false
        }
    }
}